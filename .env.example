# OpenAI API Key for the backend chat functionality
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Model to use (defaults to gpt-4-turbo if not specified)
OPENAI_MODEL=gpt-4-turbo

# CopilotKit API Key for the frontend CopilotKit functionality
NEXT_PUBLIC_COPILOT_API_KEY=your_copilotkit_api_key_here

# Optional: CopilotKit configuration
NEXT_PUBLIC_COPILOT_API_ENDPOINT=/api/copilot/chat
